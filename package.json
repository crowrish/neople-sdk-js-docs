{
  "name": "neople-sdk-js-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn build-search && next build",
    "build-search": "tsx scripts/build-search-index.ts",
    "dev": "yarn build-search && next dev --turbo",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "check-all": "npm run type-check && npm run lint:check && npm run format:check"
  },
  "dependencies": {
    "@types/react-syntax-highlighter": "^15.5.13",
    "es-hangul": "^2.3.5",
    "fumadocs-core": "15.6.9",
    "fumadocs-mdx": "11.7.3",
    "fumadocs-ui": "15.6.9",
    "hangul-search-js": "^1.0.5",
    "lucide-react": "^0.539.0",
    "minisearch": "^7.1.2",
    "neople-sdk-js": "0.2.6",
    "next": "15.4.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-syntax-highlighter": "^15.6.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/mdx": "^2.0.13",
    "@types/node": "24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "eslint": "^8",
    "eslint-config-next": "15.4.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "gray-matter": "^4.0.3",
    "neople-openapi-types": "1.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
